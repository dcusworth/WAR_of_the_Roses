    "#make lists of winners' ages and all contestants' ages\n",
    "winnerAges = []\n",
    "allAges = []\n",
    "for d in listAllDicts:\n",
    "    if (d['season'] > 12):       #for seasons 13 forward,\n",
    "        if d['elimination'] == 'Winner':\n",
    "            winnerAges.append(int(d['age']))\n",
    "        else:\n",
    "            allAges.append(int(d['age']))\n",
    "\n",
    "#plot ages in histogram\n",
    "fig = plt.figure()\n",
    "ax = fig.add_subplot(1,1,1)\n",
    "ax.hist(allAges, bins=np.arange(20, 36+2, 2), alpha=0.25, label='All Contestants')\n",
    "ax.hist(winnerAges, bins=np.arange(20, 36+2, 2), alpha=0.75, label='Winners')\n",
    "\n",
    "#add legend and labels\n",
    "ax.legend()\n",
    "ax.set_title(\"Contestants' Ages\", fontsize=28)\n",
    "ax.set_ylabel(\"Count\", fontsize=22)\n",
    "ax.set_xlabel(\"Ages\", fontsize=22)\n"
   ]
